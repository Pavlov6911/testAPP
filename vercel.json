{
  "version": 2,
  "buildCommand": "npm install --force && npm run build",
  "installCommand": "npm install --force",
  "outputDirectory": "build",
  "framework": "create-react-app",
  "routes": [
    {
      "src": "^/static/(.*)",
      "dest": "/static/$1"
    },
    {
      "src": "^/favicon.ico",
      "dest": "/favicon.ico"
    },
    {
      "src": "^/manifest.json",
      "dest": "/manifest.json"
    },
    {
      "src": "^/.*",
      "dest": "/index.html"
    }
  ]
}
